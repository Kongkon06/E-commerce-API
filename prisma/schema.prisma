generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Products{
  id Int @id @default(autoincrement()) 
  product_name String
  price String

}
model User{
  id  Int @id @default(autoincrement()) 
  name String
  email String @unique
  phone String @unique
  password String
}

model Admin{
  id Int @id @default(autoincrement())
  name String
  email String @unique
  password String 
}